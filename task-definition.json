{
  "family": "massariol",
  "executionRoleArn": "arn:aws:iam::AKIA4ER5OYJ7LANRLQOC:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "massariol",
          "image": "834426290814.dkr.ecr.sa-east-1.amazonaws.com/ecr-massariol-repo:499a81aee82fb4bce9634b151edcadf392914cd5",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 8080,
              "protocol": "tcp",
              "containerPort": 8080
            }
          ],
          "memory": 512,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/massariol",
              "awslogs-region": "sa-east-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "Bridge",
  "cpu": "1 vCPU",
  "memory": "1GB"
}
